{
  "version": 2,
  "builds": [
    {
      "src": "src/app/layout.tsx",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app/layout.tsx"
    }
  ],
  "env": {
    "NEXT_PUBLIC_LOCAL_API_URL": "https://u-tube-apis.vercel.app/api/"
  }
}
