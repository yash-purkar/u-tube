{
  "version": 2,
  "builds": [
    {
      "src": "layout.tsx",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install --legacy-peer-deps"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/layout.tsx"
    }
  ],
  "env": {
    "NEXT_PUBLIC_LOCAL_API_URL": "https://u-tube2.vercel.app/api/"
  }
}
